{"ast":null,"code":"import axios from \"axios\";\nimport { onMounted, reactive, ref } from \"vue\";\nexport default {\n  //eslint-disable-next-line\n  name: \"MyPage\",\n  prop: {\n    one: Number\n  },\n  //   setup(props, context) {\n  setup(props) {\n    //created랑 비슷하다\n\n    //data\n    let follower = ref([]); //primitive 타입에 사용\n    let test = reactive({\n      name: 'kim'\n    }); //ref랑 동일한 역할. object, array에 사용\n    test;\n    onMounted(() => {\n      axios.get(\"/follower.json\").then(res => {\n        follower.value = res.data;\n      });\n    });\n    return {\n      follower\n    };\n  },\n  data() {\n    return {};\n  }\n};","map":{"version":3,"names":["axios","onMounted","reactive","ref","name","prop","one","Number","setup","props","follower","test","get","then","res","value","data"],"sources":["C:\\Users\\ghdgj\\Desktop\\vue\\vue_study\\codingApple\\vuestagram\\src\\components\\MyPage.vue"],"sourcesContent":["<template>\r\n  <div style=\"padding: 10px\">\r\n    <h4>팔로워</h4>\r\n    <input placeholder=\"?\" />\r\n    <div class=\"post-header\" v-for=\"items,i in follower\" :key=\"i\">\r\n      <div class=\"profile\" :style=\"`background-image : url(${items.image})`\"></div>\r\n      <span class=\"profile-name\">{{ items.name }}</span>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { onMounted, reactive, ref } from \"vue\";\r\nexport default {\r\n  //eslint-disable-next-line\r\n  name: \"MyPage\",\r\n  prop:{\r\n    one:Number\r\n  },\r\n//   setup(props, context) {\r\n    setup(props) {\r\n    //created랑 비슷하다\r\n\r\n    //data\r\n    let follower = ref([]);//primitive 타입에 사용\r\n    let test = reactive({name:'kim'})//ref랑 동일한 역할. object, array에 사용\r\n    test; \r\n\r\n\r\n    onMounted(() => {\r\n      axios.get(\"/follower.json\").then((res) => {\r\n        follower.value = res.data;\r\n      });\r\n    });\r\n\r\n    return { follower };\r\n  },\r\n  data() {\r\n    return {};\r\n  },\r\n};\r\n</script>\